{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "backend/src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/src/main.ts"
      }
    ],
    "env": {
      "NEXT_PUBLIC_API_PROXY_TARGET": "https://slapking99.com"
    }
  }
  